<Window x:Class="NordChecker.Views.LoadProxiesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NordChecker.Views"
        xmlns:System="clr-namespace:System;assembly=mscorlib"
        xmlns:models="clr-namespace:NordChecker.Models"
        xmlns:xnet="clr-namespace:Leaf.xNet;assembly=Leaf.xNet"
        xmlns:hc="https://handyorg.github.io/handycontrol"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type=models:AppSettings, IsDesignTimeCreatable=True}"
        WindowStartupLocation="CenterScreen"
        Title="Загрузить прокси" Height="450" Width="800">
    <Window.Resources>
        <ObjectDataProvider x:Key="ProxyTypeEnumValues" MethodName="GetValues" ObjectType="{x:Type System:Enum}">
            <ObjectDataProvider.MethodParameters>
                <x:Type TypeName="xnet:ProxyType"/>
            </ObjectDataProvider.MethodParameters>
        </ObjectDataProvider>
    </Window.Resources>
    <Grid>
        <hc:ComboBox ItemsSource="{Binding Source={StaticResource ProxyTypeEnumValues}}" SelectedValue="{Binding Settings.LastChosenProxyType}" Grid.Column="2"/>
    </Grid>
</Window>
